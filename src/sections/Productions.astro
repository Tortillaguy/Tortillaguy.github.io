---
import Card from "../components/Card.astro";
import productions from "../data/productions";
---

<div class="my-[50px]">
  <div class="flex justify-center w-full m-auto mb-5 prose">
    <h2 id={"productions"} class="m-0 text-white text-shadow">Productions</h2>
  </div>
  <div id="project-list" class="self-start justify-start gap-2 card-list">
    {productions.map((project) => <Card project={project} />)}
  </div>
</div>
